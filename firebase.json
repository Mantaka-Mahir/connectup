{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "YOUR_PROJECT_ID",
          "appId": "YOUR_APP_ID",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "YOUR_PROJECT_ID",
          "configurations": {
            "android": "YOUR_ANDROID_APP_ID",
            "web": "YOUR_WEB_APP_ID",
            "windows": "YOUR_WINDOWS_APP_ID"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
